{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "aarch32-rpi-remote-debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/kernel/vendor/rpi/kernel.zwerg",
      "miDebuggerPath": "${workspaceRoot}/scripts/opt/cross/bin/arm-none-eabi-gdb",
      "MIMode": "gdb",
      "cwd": "${workspaceRoot}/build/kernel/vendor/rpi/",
      "setupCommands": [
        {
          "text": "set print pretty on"
        },
        {
          "text": "file ${workspaceRoot}/build/kernel/vendor/rpi/kernel.sym"
        },
        {
          "text": "set architecture arm"
        },
        {
          "text": "set serial baud 115200"
        },
        {
          "text": "target remote /dev/ttyUSB0"
        }
      ],
      "externalConsole": false,
      "logging": {
        "engineLogging": true,
        "traceResponse": true,
        "programOutput": true,
        "trace": true,
        "exceptions": true,
        "moduleLoad": true
      }
    }
  ]
}
