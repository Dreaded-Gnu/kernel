{
  "files.associations": {
    "*.h": "c",
    "*.c": "c",
    "src/arch/arm/**/**.S": "arm",
    "platform/rpi/**/**.S": "arm"
  },
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "editor.insertSpaces": true,
  "workbench.colorTheme": "Default Dark+",
  "editor.rulers": [
    80, 120
  ],
  "files.insertFinalNewline": true,
  "C_Cpp.loggingLevel": "Error"
}
