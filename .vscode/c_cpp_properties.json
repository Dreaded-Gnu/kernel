{
  "env": {
    "kernelDefaultIncludePaths": [
      "${workspaceFolder}/include",
      "${workspaceFolder}/include/lib",
      "${workspaceFolder}/include/kernel"
    ],
    "kernelDefaultBrowsePaths": [
      "${workspaceFolder}/src",
      "${workspaceFolder}/include",
      "${workspaceFolder}/include/lib",
      "${workspaceFolder}/include/kernel"
    ]
  },
  "configurations": [
    {
      "name": "Mac",
      "includePath": [
        "${kernelDefaultIncludePaths}"
      ],
      "defines": [],
      "intelliSenseMode": "${default}",
      "browse": {
        "path": [
          "${kernelDefaultBrowsePaths}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "macFrameworkPath": [],
      "forcedInclude": [
        "${workspaceFolder}/build/include/config.h"
      ],
      "cStandard": "c11"
    },
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/include/lib",
        "${workspaceFolder}/include/kernel"
      ],
      "defines": [],
      "intelliSenseMode": "${default}",
      "browse": {
        "path": [
          "${workspaceFolder}/src",
          "${workspaceFolder}/include",
          "${workspaceFolder}/include/lib",
          "${workspaceFolder}/include/kernel"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "forcedInclude": [
        "${workspaceFolder}/build/include/config.h"
      ],
      "cStandard": "c11"
    },
    {
      "name": "Win32",
      "includePath": [
        "${kernelDefaultIncludePaths}"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE"
      ],
      "intelliSenseMode": "${default}",
      "browse": {
        "path": [
          "${kernelDefaultBrowsePaths}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "forcedInclude": [
        "${workspaceFolder}/build/include/config.h"
      ],
      "cStandard": "c11"
    }
  ],
  "version": 4
}
